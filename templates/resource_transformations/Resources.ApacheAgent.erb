<resourceDescription xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <bindings xdt:Locator="XPath(//bindings[moduleList/module/@name='Atomia.Provisioning.Modules.Apache.Apache'])">
    <resourceList>
      <resource name="apache" xdt:Transform="Insert">
        <property name="URL">http://<%= @apache_url %>:9999/ApacheAgentService?wsdl</property>
        <property name="Username"><%= @apache_username %></property>
        <property name="Password"><%= @apache_password %></property>
        <property name="ClusterIpAddress"><%= @apache_clusterIp %></property>
      </resource>
    </resourceList>
  </bindings>
</resourceDescription>
