[settings]
SERVER_WEB_ROOT             = /storage/content
VHOSTS_MAP_FILE             = <%= @maps_path %>/vhost.map
USERS_MAP_FILE              = <%= @maps_path %>/users.map
PARKING_MAP_FILE            = <%= @maps_path %>/parks.map
REDIRECTION_MAP_FILE        = <%= @maps_path %>/redrs.map
FRAME_REDIRECTION_MAP_FILE  = <%= @maps_path %>/frmrs.map
SUSPENDED_MAP_FILE          = <%= @maps_path %>/sspnd.map
PACKAGE_MAP_FILE            = <%= @maps_path %>/packg.map
PHPVERSION_MAP_FILE         = <%= @maps_path %>/phpvr.map
SERVICE_PORT                = 9999
SSL_CONFIG_DIR              = /etc/apache2/ssl.d
SSL_CERT_DIR                = /storage/configuration/ssl
APACHECTL_PATH              = /usr/sbin/apache2ctl
SERVICE_AUTH                = True
SERVICE_AUTH_USER           = <%= @username %>
SERVICE_AUTH_PW             = <%= @password %>
SERVE_HTTPS                 = False
CERTIFICATE_FILE            = /usr/local/apache-agent/wildcard.crt
PRIVATE_KEY_FILE            = /usr/local/apache-agent/wildcard.key
